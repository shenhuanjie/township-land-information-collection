<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" android:layout_width="match_parent"
	android:layout_height="match_parent" android:orientation="vertical">

	<LinearLayout style="@style/gardensLinearLayout" android:layout_marginBottom="5dp"
		android:layout_marginLeft="10dp" android:layout_marginRight="10dp">

		<TextView style="@style/gardensLeft" android:text="转让方" />

		<EditText android:id="@+id/supply_transfer_farm_in_ZRF" style="@style/gardensRight" />
	</LinearLayout>

	<LinearLayout style="@style/gardensLinearLayout" android:layout_marginBottom="5dp"
		android:layout_marginLeft="10dp" android:layout_marginRight="10dp">

		<TextView style="@style/gardensLeft" android:text="拟转让时间" />

		<EditText android:id="@+id/supply_transfer_farm_in_NZRSJ" style="@style/gardensRight" />
	</LinearLayout>

	<LinearLayout style="@style/gardensLinearLayout" android:layout_marginBottom="10dp"
		android:layout_marginLeft="10dp" android:layout_marginRight="10dp">

		<TextView style="@style/gardensLeft" android:text="拟转让方式" />

		<Spinner android:id="@+id/supply_transfer_farm_in_NZRFS" style="@style/gardensRight" android:entries="@array/tdjb" />
	</LinearLayout>

	<LinearLayout style="@style/gardensLinearLayout" android:layout_marginBottom="10dp"
		android:layout_marginLeft="10dp" android:layout_marginRight="10dp">

		<TextView style="@style/gardensLeft" android:text="拟土地转让面积" />

		<LinearLayout style="@style/gardensRight">

			<EditText android:id="@+id/supply_transfer_farm_in_NTDZRMJ" style="@style/gardensRight"
				android:layout_weight="15" android:inputType="numberDecimal" />

			<TextView style="@style/gardensRight" android:layout_weight="80" android:text="平方米" />
		</LinearLayout>
	</LinearLayout>

	<LinearLayout style="@style/gardensLinearLayout" android:layout_marginBottom="5dp"
		android:layout_marginLeft="10dp" android:layout_marginRight="10dp">

		<TextView style="@style/gardensLeft" android:text="转让前用途" />

		<EditText android:id="@+id/supply_transfer_farm_in_ZRQYT" style="@style/gardensRight" />
	</LinearLayout>

	<LinearLayout style="@style/gardensLinearLayout" android:layout_marginBottom="5dp"
		android:layout_marginLeft="10dp" android:layout_marginRight="10dp">

		<TextView style="@style/gardensLeft" android:text="拟转让后用途" />

		<EditText android:id="@+id/supply_transfer_farm_in_NZRHYT" style="@style/gardensRight" />
	</LinearLayout>

	<LinearLayout style="@style/gardensLinearLayout" android:layout_marginBottom="5dp"
		android:layout_marginLeft="10dp" android:layout_marginRight="10dp">

		<TextView style="@style/gardensLeft" android:text="土地剩余使用年期" />

		<EditText android:id="@+id/supply_transfer_farm_in_TDSYSYNQ" style="@style/gardensRight" />
	</LinearLayout>

	<LinearLayout style="@style/gardensLinearLayout" android:layout_marginBottom="10dp"
		android:layout_marginLeft="10dp" android:layout_marginRight="10dp">

		<TextView style="@style/gardensLeft" android:text="拟土地转让单价" />

		<LinearLayout style="@style/gardensRight">

			<EditText android:id="@+id/supply_transfer_farm_in_NTDZRDJ" style="@style/gardensRight"
				android:layout_weight="15" android:inputType="numberDecimal" />

			<TextView style="@style/gardensRight" android:layout_weight="80" android:text="元/平方米" />
		</LinearLayout>
	</LinearLayout>
	<LinearLayout style="@style/gardensLinearLayout" android:layout_marginBottom="10dp"
		android:layout_marginLeft="10dp" android:layout_marginRight="10dp">

		<TextView style="@style/gardensLeft" android:text="拟土地转让总价" />

		<LinearLayout style="@style/gardensRight">

			<EditText android:id="@+id/supply_transfer_farm_in_NTDZRZJ" style="@style/gardensRight"
				android:layout_weight="15" android:inputType="numberDecimal" />

			<TextView style="@style/gardensRight" android:layout_weight="80" android:text="万元" />
		</LinearLayout>
	</LinearLayout>
</LinearLayout>